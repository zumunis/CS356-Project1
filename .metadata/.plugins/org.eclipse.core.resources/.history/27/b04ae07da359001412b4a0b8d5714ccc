
public class Question {
private String quest;
private String answer;
private String[] options;
public Question(){ }
public String getQuest() {
	return quest;
}
public void setQuest(String quest) {
	this.quest = quest;
}
public String getAnswer() {
	return answer;
}
public void setAnswer(String answer) {
	this.answer = answer;
}

}