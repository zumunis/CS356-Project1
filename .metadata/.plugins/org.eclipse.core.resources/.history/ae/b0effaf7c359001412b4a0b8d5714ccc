import java.util.ArrayList;


public class Student {
	private int ID;
	private ArrayList<String> answers = new ArrayList<String>();
	public Student()
	{
		this.ID = 0;
		this.answers=null;
	}
	
	public void setID(int id)
	{
		this.ID =id;
	}
	public int getID()
	{
		return this.ID;
	}
	
	public void updateAns(String ans)
	{
		answers.add(ans);
	}
	
	public ArrayList<String> getAns()
	{
		return this.answers;
	}

}
