	public Student[] initializeStudents(Student[] S)
	{
		int uniqueID = generateUniqueID(S);
		for(int i=0; i<S.length; i++)
		{
			S[i].setID(uniqueID);
			uniqueID = generateUniqueID(S);
		}
		return S;
	}

